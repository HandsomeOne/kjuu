{
  "name": "kjuu",
  "version": "0.7.0",
  "description": "kjuu (Q) for reQuest",
  "keywords": [],
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "author": "Zhou Qi <i@handsomeone.com>",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watchAll",
    "start": "docz dev",
    "doc": "docz build",
    "prebuild": "eslint \"src/**\" && rimraf lib",
    "build": "tsc",
    "prepublishOnly": "npm run test && npm run build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "setupFiles": [
      "./test/init/fetchMock.ts"
    ],
    "collectCoverage": true
  },
  "devDependencies": {
    "@types/fetch-mock": "^7.2.3",
    "@types/jest": "^24.0.9",
    "@types/node": "^11.10.4",
    "@typescript-eslint/eslint-plugin": "^1.4.2",
    "@typescript-eslint/parser": "^1.4.2",
    "docz": "^0.13.7",
    "docz-theme-default": "^0.13.7",
    "eslint": "^5.15.1",
    "eslint-config-prettier": "^4.1.0",
    "fetch-mock": "^7.3.1",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "node-fetch": "^2.3.0",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.10.0",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.0",
    "typescript": "^3.3.3333"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HandsomeOne/kjuu.git"
  },
  "bugs": {
    "url": "https://github.com/HandsomeOne/kjuu/issues"
  },
  "homepage": "https://github.com/HandsomeOne/kjuu#readme"
}
